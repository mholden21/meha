def gaussian (x, amp, mu, sigma):
    return amp * np.exp(-0.5 * ((x - mu) / sigma)**2)

p0 = [1e-16, 13350, 10, 5e-16, 13450, 10, 8e-16, 13550, 10]


popt, pcov = curve_fit(gaussian, x, y)



